/* 2.10. Операции с присваиванием и выражения с ними */
//Упражнение 2.9

#include <stdio.h>
#include <math.h>

int bitcount (unsigned x);

int main()
{
	char c;

	int s = 0x1FF;


	printf("%d", bitcount(s));
	c = getchar();
    return 0;
}

int bitcount (unsigned x){
	int b;
	for (b = 0; x != 0; x &= (x-1) )
		b++;

	return b;
}
