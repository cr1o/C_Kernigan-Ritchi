#include <stdio.h>

#define MAX_LINE 1000
int strend(char *s, char *t );

int main (){
	//int i;
	char s1[MAX_LINE] = "ABCDEFG";
	char s2[MAX_LINE] = "FFG";
	printf("Line1 = %s\n", s1);
	printf("Line2 = %s\n", s2);
	printf("Result = %d\n", strend(s1, s2) );
	getchar();
	return 0;
}

int strend(char *s, char *t ){
	char *sp = s;
	char *tp = t;

	while (*s++ != '\0')
		;
	while (*t++ != '\0')
		;
	printf("Length of s = %d\n", s - sp  );
	printf("Length of t = %d\n", t - tp  );

	s = sp + (s - sp) - (t - tp);
	t = tp;
	printf("*s = %c\n", *s  );

	while (*s++ == *t++)
		if (*s == '\0')
			return 1;
	return 0;
}
